#######################
######## CURSO ########
#######################


# 1. Adicionar

expecterror "Atributo inválido" addCurso sigla="" nome="Tecnólogo em Construção de Edifícios"
expecterror "Atributo inválido" addCurso sigla="" nome="Análise e Desenvolvimento de Sistemas"
expecterror "Atributo inválido" addCurso sigla="ADS" nome=""
expecterror "Atributo inválido" addCurso sigla="TCE" nome=

addCurso sigla="ADS" nome="Análise e Desenvolvimento de Sistemas"
addCurso sigla="TCE" nome="Tecnólogo em Construção de Edifícios"
addCurso sigla="TT" nome="Teste Teste"
addCurso sigla="CS" nome="Counter Strike"

# 2. Alterar

expecterror "Atributo inválido" alterarCurso identificadora="" atributo="DASDAS"
expecterror "Atributo inválido" alterarCurso identificador= atributo="sigla"
expecterror "Atributo inválido" alterarCurso identificador="CS" atributo=""
expecterror "Atributo inválido" alterarCurso identificador="TCE" atributo=""
expecterror "Atributo inválido" alterarCurso identificador="ADS" atributo=
expecterror "Atributo inválido" alterarCurso identificador= atributo="D"

#expecterror "Atributo inválido" alterarCurso identificador="TT" atributo="sigla"
#expecterror "Atributo inválido" alterarCurso identificador="CS" atributo="nomes"
#expecterror "Atributo inválido" alterarCurso identificador="CS" atributo="no_me"


expecterror "Curso não cadastrado" alterarCurso identificador="CC" atributo="D"
expecterror "Curso não cadastrado" alterarCurso identificador="TC_E" atributo="D"
expecterror "Curso não cadastrado" alterarCurso identificador="ADSS" atributo="A"
expecterror "Curso não cadastrado" alterarCurso identificador="CSC" atributo="B"

expect "ADS - Análise e Desenvolvimento de Sistemas" getCurso idCurso="ADS"
alterarCurso identificador="ADS" novoValor="Análise e Desenv. de Sist."
expect "ADS - Análise e Desenv. de Sist." getCurso idCurso="ADS"

#expect "CS - Counter Strike" getCurso idCurso="CS"
#alterarCurso sigla="CS" atributo="sigla" novoValor="ICS"
#alterarCurso sigla="ICS" atributo="nome" novoValor="Introdução à Counter Strike"
#expect "ICS - Introdução à Counter Strike" getCurso idCurso="ICS"

expect "TCE - Tecnólogo em Construção de Edifícios" getCurso idCurso="TCE"
alterarCurso identificador="TCE" novoValor="Tec. em Const. de Edif."
expect "TCE - Tec. em Const. de Edif." getCurso idCurso="TCE"

alterarCurso sigla="TCE" novoValor="Tecnólogo em Construção de Edifícios"
alterarCurso sigla="ADS" novoValor="Análise e Desenvolvimento de Sistemas"



# 3. Remover

removeCurso identificador="CS"

expecterror "Atributo inválido" removeCurso nome=""
expecterror "Atributo inválido" removeCurso nome=

expecterror "Curso não cadastrado" removeCurso idCurso="TC_E"
expecterror "Curso não cadastrado" removeCurso idCurso="AD_S"
expecterror "Curso não cadastrado" removeCurso idCurso="I.CS"
expecterror "Curso não cadastrado" removeCurso idCurso="ADSS"
expecterror "Curso não cadastrado" removeCurso idCurso="ICCS"
expecterror "Curso não cadastrado" removeCurso idCurso="TTT"

removeCurso identificador="TT"
expecterror "Curso não cadastrado" removeCurso idCurso="TT"

#removeCurso nome="ICS"
expecterror "Curso não cadastrado" removeCurso nome="ICS"



# 4. Recuperando Arquivos

expecterror "Atributo inválido" getCurso idCurso=""
expecterror "Atributo inválido" getCurso idCurso=

expecterror "Curso não cadastrado" getCurso idCurso="TT"
expecterror "Curso não cadastrado" getCurso idCurso="ICS"
expecterror "Curso não cadastrado" getCurso idCurso="AD_S"
expecterror "Curso não cadastrado" getCurso idCurso="ADSS"
expecterror "Curso não cadastrado" getCurso idCurso="TCCE"

expect "ADS - Análise e Desenvolvimento de Sistemas" getCurso idCurso="ADS"
expect "TCE - Tecnólogo em Construção de Edifícios" getCurso idCurso="TCE"



