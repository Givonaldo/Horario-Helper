expecterror "Atributo inválido" addProfessor idProfessor="" nome=

addProfessor idProfessor="12324" nome="Mirna Maia"
addProfessor idProfessor="9999" nome="Hugo"
addProfessor idProfessor="444" nome="Bruno Luna"
addProfessor idProfessor="333" nome="Maria José"
addProfessor idProfessor="111" nome="José"
addProfessor idProfessor="222" nome="Marcela"

expecterror "Professor já cadastrado" addProfessor idProfessor="12324" nome="Mirna Maia"
expecterror "Professor já cadastrado" addProfessor idProfessor="9999" nome="Hugo"




# 2. Alterar


expecterror "Atributo inválido" alteraProfessor idProfessor="" novoNome="Jeferson"
expecterror "Atributo inválido" alteraProfessor idProfessor="" novoNome="Solange"
expecterror "Atributo inválido" alteraProfessor idProfessor= novoNome="José"
expecterror "Atributo inválido" alteraProfessor idProfessor="111" novoNome=""
expecterror "Atributo inválido" alteraProfessor idProfessor="111" novoNome=
expecterror "Atributo inválido" alteraProfessor idProfessor="111" novoNome=""
expecterror "Atributo inválido" alteraProfessor idProfessor= novoNomer="José"
expecterror "Atributo inválido" alteraProfessor idProfessor="444" novoNome="&&()askdnnl"
expecterror "Atributo inválido" alteraProfessor idProfessor="333" novoNome=


# 3. Remover

expecterror "Professor não Cadastrado" removeProfessor idProfessor="12345"
expecterror "Professor não Cadastrado" removeProfessor idProfessor="99778"

removeProfessor idProfessor="444"
removeProfessor idProfessor="333"
expecterror "Professor não Cadastrado" removeProfessor idProfessor="333"
expecterror "Professor não Cadastrado" removeProfessor idProfessor="444"



# 4. Recuperar arquivos

expecterror "Atributo inválido" getProfessor idProfessor=""
expecterror "Atributo inválido" getProfessor idProfessor=
#expecterror "Professor não cadastrado" getProfessor matricula="3333"
#expecterror "Professor não cadastrado" getProfessor matricula="444"

expect "111 - José" getProfessor idProfessor="111"
expect "222 - Marcela" getProfessor idProfessor="222"

removeProfessor idProfessor="111"
expecterror "Professor não cadastrado" getProfessor idProfessor="111"

addProfessor idProfessor="1234" nome="Mirna Maia"
expect "1234 - Mirna Maia" getProfessor idProfessor="1234"



